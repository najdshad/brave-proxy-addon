{
  "manifest_version": 3,
  "name": "Proxy Switcher",
  "version": "1.0",
  "description": "Minimal proxy switcher for Brave",
  "permissions": ["proxy", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Proxy Switcher"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "256": "assets/icon256.png"
  }
}
